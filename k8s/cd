pipeline {
  agent any
   
    stages {
	    
	  stage('Deploy Production') {
            steps{
            	sh 'ls k8s/app/'
                step([$class: 'KubernetesEngineBuilder', 
                        projectId: "mostafa-ashour-project",
                        clusterName: "primary",
                        zone: "us-central1-a",
                        manifestPattern: 'k8s/app/python-deploy.yaml',
                        credentialsId: "mostafa-ashour-project",
                        verifyDeployments: true])
            }
        } 
	}
}





















